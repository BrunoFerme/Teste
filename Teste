<script>
    var prm = Sys.WebForms.PageRequestManager.getInstance();
    prm.add_pageLoaded(function () {

        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini|windows phone/i.test(navigator
            .userAgent)) {
            jQuery('.link-whatsapp').attr('href',
                'https://api.whatsapp.com/send?phone=+351926015160&text=Consegue+ligar-me,+por+favor+?');
        } else {
            jQuery('.link-whatsapp').attr('href',
                'https://web.whatsapp.com/send?phone=+351926015160&text=Consegue ligar-me, por favor ?');
        }

        function fechaModal() {
            sessionStorage.setItem('alreadyShow', 'alredy shown');
            $("#myModal").css("display", "none");
        }


        $(document).ready(function () {
            var date = new Date("2022-12-27 23:00");
            var endDateSecs = Date.parse(date);
            var startdate = new Date("2022-12-05 23:00");
            var startDateSecs = Date.parse(startdate);

            var currentDate = new Date();
            var schCurrentSecs = Date.parse(currentDate);
            var is_modal_show = sessionStorage.getItem('alreadyShow');

            console.log("Aki11 current  " + schCurrentSecs)
            console.log("Aki11 end  " + endDateSecs)

            console.log("Aki11 start  " + endDateSecs)
            console.log(is_modal_show);

            if (schCurrentSecs < endDateSecs && schCurrentSecs > startDateSecs) {
                //show modal
                if (is_modal_show != 'alredy shown') {
                    console.log("mostra");
                    $("#myModal").css("display", "block");
                    sessionStorage.setItem('alreadyShow', 'alredy shown');
                }
            }
        });

    });
</script>




