    var link = document.createElement("link");
    link.id = "FThasadit";
    link.rel = "preconnect";
    link.href = "https://fonts.gstatic.com";
    document.head.appendChild(link);

    var link1 = document.createElement("link");
    link1.id = "Thasaditcss";
    link1.rel = "stylesheet";
    link1.href =
        "https://fonts.googleapis.com/css2?family=Thasadith:ital,wght@0,400;0,700;1,400;1,700&amp;display=swap";
    document.head.appendChild(link1);

    function submitPesq() {
        console.log("Submit pesq");
        var txt = document.getElementById("txtSearch").value;
        if (txt.length > 0) {
            var urlpesq =
                "../programs/ewpview.aspx?codigo=res_pesquisa&search_txt=" + txt;
            window.location.href = urlpesq;
        }
    }

    document
        .querySelector("#txtSearch")
        .addEventListener("keypress", function (e) {
            if (e.key === "Enter") {
                event.preventDefault();
                var txt = document.getElementById("txtSearch").value;
                if (txt.length > 0) {
                    var urlpesq =
                        "http://www.amontravip.com/programs/ewpview.aspx?codigo=res_pesquisa&search_txt=" +
                        txt;
                    window.location.href = urlpesq;
                }
            }
        });
</script>




